{
  "exclude": ["./sw/**/*"],
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "lib": ["esnext", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    // "jsxFactory": "h",
    // "jsxFragmentFactory": "Fragment",
    "incremental": true
  }
}
